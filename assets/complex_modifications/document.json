{
  "title": "Document-related mappings",
  "rules": [
    {
      "description": "Shift?+Cmd+Home ↦ Shift?+Cmd+Up: Move/Select to document start",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "home",
            "modifiers": {
              "mandatory": [
                "command"
              ],
              "optional": [
                "caps_lock",
                "shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "up_arrow",
              "modifiers": [
                "command"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Shift?+Cmd+End ↦ Shift?+Cmd+Down: Move/Select to document end",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "end",
            "modifiers": {
              "mandatory": [
                "command"
              ],
              "optional": [
                "caps_lock",
                "shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "down_arrow",
              "modifiers": [
                "command"
              ]
            }
          ]
        }
      ]
    }
  ]
}